{
  "icon": "extension-icon.svg",
  "host": {
    "binaries": [
      {
        "darwin": [
          {
            "path": "/bin/dummy.sh"
          }
        ],
        "windows": [
          {
            "path": "/bin/dummy.exe"
          }
        ],
        "linux": [
          {
            "path": "/bin/dummy.sh"
          }
        ]
      }
    ]
  },
  "ui": {
    "dashboard-tab": {
      "title": "Sample Extension With Everything",
      "root": "/ui",
      "src": "index.html",
      "backend": {
        "socket": "hello.sock"
      }
    }
  },
  "vm": {
    "composefile": "/compose/compose.yaml",
    "exposes": {
      "socket": "hello.sock"
    }
  }
}
